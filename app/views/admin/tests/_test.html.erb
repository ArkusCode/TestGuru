<tr>
  <td><%= test.title %></td>
  <td><%= test_level(test) %></td>
  <td><%= test.questions.count %></td>
  <td><%= link_to t('admin.show_link'), admin_test_path(test) %></td>
  <td><%= link_to t('admin.edit_link'), edit_admin_test_path(test) %></td>
  <td>
    <%= link_to t('admin.delete_link'),
                admin_test_path(test),
                method: :delete,
                data: { confirm: t('admin.confirm') } %>
  </td>
</tr>
